{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "./../../env.d.ts",
    "./../../src/**/*",
    "./../../src/**/*.vue",
    "./**/*",
    "../support/**/*"
  ],
  "compilerOptions": {
    "isolatedModules": false,
    "baseUrl": ".",
    "target": "es5",
    "lib": ["es5", "dom"],
    "types": ["cypress"],
    "paths": {
      "@/*": ["./../../src/*"]
    }
  }
}
